                                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" textAlignment="justified" translatesAutoresizingMaskIntoConstraints="NO" id="Z4c-Iw-lXG">
                                                <rect key="frame" x="13" y="17" width="278" height="241"/>
